# Auto-generated Fish shell colors (Material You theme)
# This file is sourced automatically by fish

# IMPORTANT: Use -U (universal) to override any existing universal variables
# This ensures the theme takes effect even if colors were previously set with -U

# Syntax highlighting colors
set -U fish_color_normal e8d3de
set -U fish_color_command c0adff
set -U fish_color_keyword bcbaff
set -U fish_color_quote e390ff
set -U fish_color_redirection cc92ff
set -U fish_color_end a599ff
set -U fish_color_error 65c720
set -U fish_color_param e8d3de
set -U fish_color_comment 7d6bb8
set -U fish_color_selection --background=3a1592
set -U fish_color_operator c9a1ff
set -U fish_color_escape e59d00
set -U fish_color_autosuggestion 6e658d
set -U fish_color_valid_path bfa1ff --underline
set -U fish_color_search_match --background=f397ff

# Pager (completion menu) colors
set -U fish_pager_color_prefix c7b7ff --bold
set -U fish_pager_color_completion e8d3de
set -U fish_pager_color_description 9881e6
set -U fish_pager_color_progress d68eff
set -U fish_pager_color_selected_background --background=3a1592

# Additional useful colors
set -U fish_color_cancel 65c720
set -U fish_color_cwd c0adff
set -U fish_color_cwd_root 65c720
set -U fish_color_history_current f397ff --bold
set -U fish_color_host e390ff
set -U fish_color_host_remote e390ff
set -U fish_color_match f397ff
set -U fish_color_user c0adff

# Export global variables for custom prompts to use
# These match your current prompt's color scheme
set -U material_prompt_bracket bcbaff
set -U material_prompt_username c0adff
set -U material_prompt_hostname e390ff
set -U material_prompt_path cc92ff
set -U material_prompt_git e390ff
set -U material_prompt_arrow bcbaff
